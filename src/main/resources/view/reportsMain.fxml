<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.ColorAdjust?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="372.0" prefWidth="600.0" style="-fx-background-color: grey;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.reportsMainController">
   <children>
      <Button layoutX="248.0" layoutY="100.0" mnemonicParsing="false" onAction="#onActionContactReport" style="-fx-background-color: red; -fx-border-color: white;" text="Contact Report">
         <effect>
            <ColorAdjust saturation="-0.61" />
         </effect>
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button layoutX="251.0" layoutY="270.0" mnemonicParsing="false" onAction="#onActionDivisionReport" style="-fx-background-color: blue; -fx-border-color: white;" text="Division Report" textFill="#fffbfb" />
      <Button layoutX="204.0" layoutY="185.0" mnemonicParsing="false" onAction="#onActionCustAppReport" style="-fx-background-color: white; -fx-border-color: white;" text="Customer Appointment Report">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Button layoutX="27.0" layoutY="25.0" mnemonicParsing="false" onAction="#onActionBackToMainScreen" prefHeight="25.0" prefWidth="72.0" text="&lt;-- Back">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
